---
import {GoogleFontsOptimizer as Fonts} from "astro-google-fonts-optimizer";
import {SEO} from "astro-seo";
import Navbar from "../components/nav/Navbar.astro";
import Footer from "../components/Footer.astro";
---

<html lang="en">
<head>
    <meta charset="utf-8"/>
    <link rel="icon" type="image/x-icon" href="/favicon.ico"/>
    <meta name="viewport" content="width=device-width"/>
    <meta name="generator" content={Astro.generator}/>
    <title>TutorLion</title>
    <Fonts url="https://fonts.googleapis.com/css2?family=Nunito&display=swap"/>
    <Fonts url="https://fonts.googleapis.com/css2?family=Poppins&display=swap"/>
    <SEO />
    <slot name="head" />
</head>
<body class="flex flex-col min-h-screen bg-stone-100 font-nunito">
<Navbar />
<main class="flex-1">
<slot/>
</main>
<Footer />
</body>
</html>